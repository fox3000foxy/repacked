{
    "model": {
      "type": "minecraft:select",
      "property": "minecraft:custom_model_data",
      "index": 0,
      "cases": [
        {
          "when": "gui",
          "model": {
            "type": "minecraft:select",
            "property": "minecraft:custom_model_data",
            "index": 1,
            "cases": [
              {
                "when": "none",
                "model": {
                  "type": "minecraft:model",
                  "model": "expansion:blocks/enhancer/gui/none"
                }
              },
              {
                "when": "weapon",
                "model": {
                  "type": "minecraft:model",
                  "model": "expansion:blocks/enhancer/gui/weapon"
                }
              },
              {
                "when": "equipment",
                "model": {
                  "type": "minecraft:model",
                  "model": "expansion:blocks/enhancer/gui/equipment"
                }
              }
            ],
            "fallback": {
              "type": "minecraft:model",
              "model": "expansion:blocks/enhancer/gui/none"
            }
          }
        }
      ],
      "fallback": {
        "type": "minecraft:model",
        "model": "expansion:blocks/enhancer/enhancer"
      }
    },
    "oversized_in_gui": true
  }