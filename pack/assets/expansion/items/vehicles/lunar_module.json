{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:display_context",
    "cases": [
      {
        "when": "gui",
        "model": {
          "type": "minecraft:model",
          "model": "expansion:vehicles/lunar_module/icon"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:select",
      "property": "minecraft:custom_model_data",
      "index": 0,
      "cases": [
        {
          "when": "top",
          "model": {
            "type": "minecraft:model",
            "model": "expansion:vehicles/lunar_module/top"
          }
        },
        {
          "when": "bottom",
          "model": {
            "type": "minecraft:model",
            "model": "expansion:vehicles/lunar_module/bottom"
          }
        }
      ],
      "fallback": {
        "type": "minecraft:model",
        "model": "expansion:vehicles/lunar_module/lunar_module"
      }
    }
  }
}